{
  "name": "terminalzoom",
  "displayName": "TerminalZoom",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:terminalFontSize.decrease",
    "onCommand:terminalFontSize.increase",
    "onCommand:terminalFontSize.openQuickPick"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "terminalFontSize.decrease",
        "title": "Decrease terminal font size"
      },
      {
        "command": "terminalFontSize.increase",
        "title": "Increase terminal font size"
      },
      {
        "command": "terminalFontSize.openQuickPick",
        "title": "Set terminal font size"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  }
}
